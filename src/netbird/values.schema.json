        "issuer": {
          "default": "",
          "minLength": 2,
          "required": [],
          "title": "issuer"
        },
        "jwksURI": {
          "default": "",
          "description": "JWKs URI. If configEndpoint is set, this value will be ignored.",
          "required": [],
          "title": "jwksURI",
          "type": "string"
        },
        "scopes": {
          "default": "openid profile email",
          "required": [],
          "title": "scopes",
          "type": "string"
        },
        "tokenEndpoint": {
          "default": "",
          "description": "Token endpoint. If configEndpoint is set and idpManager.type is not \"none\", this value will be ignored.",
          "required": [],
          "title": "tokenEndpoint",
          "type": "string"
        }
