          "default": false,
          "required": [],
          "title": "enabled",
          "type": "boolean"
        },
        "parentRefs": {
          "items": {
            "anyOf": [
              {
                "additionalProperties": false,
                "properties": {
                  "name": {
                    "default": "",
                    "required": [],
                    "title": "name",
                    "type": "string"
