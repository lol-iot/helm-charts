        },
        "clientSecret": {
          "default": "",
          "required": [],
          "title": "clientSecret",
          "type": "string"
        },
        "configEndpoint": {
          "default": "{{ .Values.oidc.issuer }}/.well-known/openid-configuration",
          "required": [],
          "title": "configEndpoint",
          "type": "string"
        },
        "deviceAuthEndpoint": {
          "default": "",
          "description": "Device authorization endpoint. If configEndpoint is set, this value will be ignored.",
          "required": [],
          "title": "deviceAuthEndpoint",
          "type": "string"
        },
        "deviceProvider": {
          "default": "none",
          "enum": [
            "none",
            "hosted"
          ],
          "required": [],
          "title": "deviceProvider"
        },
        "idpManager": {
          "additionalProperties": false,
          "properties": {
            "extraConfig": {
              "additionalProperties": false,
              "required": [],
              "title": "extraConfig",
              "type": "object"
            },
            "type": {
              "default": "none",
              "enum": [
                "none",
                "auth0",
                "authentik",
                "azure",
                "google",
                "jumpcloud",
                "keycloak",
                "okta",
                "zitadel"
              ],
              "required": [],
              "title": "type"
            }
          },
          "required": [
            "extraConfig"
          ],
          "title": "idpManager",
          "type": "object"
