    "oidc": {
      "additionalProperties": false,
      "properties": {
        "authEndpoint": {
          "default": "",
          "description": "Authorization endpoint. If configEndpoint is set, this value will be ignored.",
          "required": [],
          "title": "authEndpoint",
          "type": "string"
        },
        "clientID": {
          "default": "",
          "minLength": 2,
          "required": [],
